<h1>Orders</h1>
<table class=".table-striped">
  <thead>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Price</th>
    <th>Hours</th>
  </tr>
  </thead>
  <tbody>
  <tr>
    <% @orders.each do |order| %>
       <td> <% order.items.each do |item|%>
            <tr>
            <td><%= item.name %></td>
            <td><%= item.description%></td>
            <td><%= item.price%></td>
            <td><%= item.hours%></td>
             <td><%= link_to order.id, edit_order_path(order.id) %></td>
        <% end %> </tr> </td>

    <% end %>
  </tr>
  </tbody>
</table>
<p><%= link_to 'New Order', new_order_path,class:"btn btn-success" %></p>